{
	"name": "@mq/ui",
	"version": "0.0.0",
	"type": "module",
	"private": true,
	"files": [
		"dist"
	],
	"exports": {
		"./styles.css": "./dist/index.css",
		"./components/": "./dist/components/",
		"./*": "./src/components/ui/*/index.tsx",
		"./json/*": "./src/components/ui/*/registry-item.json"
	},
	"scripts": {
		"dev": "bun run --bun next dev --port 6271",
		"build:styles": "bun run --bun tailwindcss -i ./src/styles/globals.css -o ./dist/index.css",
		"build": "bun run build:styles && bun run --bun next build",
		"start": "next start",
		"check-types": "next typegen && tsc --noEmit"
	},
	"dependencies": {
		"@radix-ui/react-collapsible": "^1.1.12",
		"@radix-ui/react-dropdown-menu": "^2.1.16",
		"@radix-ui/react-label": "^2.1.8",
		"@radix-ui/react-scroll-area": "^1.2.10",
		"@radix-ui/react-select": "^2.2.6",
		"@radix-ui/react-separator": "^1.1.8",
		"@radix-ui/react-slider": "^1.3.6",
		"@radix-ui/react-slot": "^1.2.4",
		"@radix-ui/react-switch": "^1.2.6",
		"@radix-ui/react-tooltip": "^1.2.8",
		"@tailwindcss/cli": "^4.1.16",
		"class-variance-authority": "^0.7.1",
		"lucide-react": "^0.553.0",
		"next": "^16.0.0",
		"next-themes": "^0.4.6",
		"radix-ui": "^1.4.3",
		"react": "catalog:",
		"react-dom": "catalog:",
		"sonner": "^2.0.7",
		"tailwindcss": "^4.1.16",
		"tw-animate-css": "^1.4.0"
	},
	"devDependencies": {
		"@mq/ts-config": "*",
		"@mq/hooks": "*",
		"@mq/lib": "*",
		"@types/node": "^22.15.3",
		"@types/react": "catalog:",
		"@types/react-dom": "catalog:",
		"typescript": "5.9.2"
	}
}
