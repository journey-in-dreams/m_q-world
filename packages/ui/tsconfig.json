{
	"extends": "@mq/ts-config/nextjs.json",
	"compilerOptions": {
		"plugins": [
			{
				"name": "next"
			}
		],
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"],
			"@ui/*": ["./src/*"]
		}
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		"next-env.d.ts",
		"next.config.js",
		".next/types/**/*.ts"
	],
	"exclude": ["node_modules"]
}
